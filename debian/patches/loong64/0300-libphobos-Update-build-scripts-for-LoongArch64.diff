+      loongarch*)
+               druntime_target_cpu_parsed="loongarch"
+               ;;
       powerpc*)
                druntime_target_cpu_parsed="powerpc"
                ;;
@@ -14174,6 +14179,14 @@ else
   DRUNTIME_CPU_MIPS_FALSE=
